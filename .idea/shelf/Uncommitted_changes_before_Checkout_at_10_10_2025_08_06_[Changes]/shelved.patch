                <td>06/05/2025</td>\r\n                    <td class=\"status-refused\">Refusée</td>\r\n                    <td>non</td>\r\n                    <td><a href=\"view_motive.html\" class=\"action-button\">Voir le motif</a></td>\r\n                </tr>\r\n                </tbody>\r\n            </table>\r\n            <div class=\"justify-button\">\r\n                <button id=\"deposit-justify-btn\" class=\"action-button\">Déposer un justificatif</button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n<footer class=\"main-footer\"></footer>\r\n\r\n<div id=\"justify-modal\" class=\"modal\">\r\n    <div class=\"modal-content\">\r\n        <div class=\"modal-header\">\r\n            <h3>Justification d'une absence</h3>\r\n            <span class=\"close-button\">&times;</span>\r\n        </div>\r\n        <div class=\"modal-body\">\r\n            <div class=\"form-section\">\r\n                <h4>Absence</h4>\r\n                <div class=\"form-group\">\r\n                    <label for=\"date-start\">Absent du</label>\r\n                    <input type=\"date\" id=\"date-start\" class=\"form-input\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"time-start\">À :</label>\r\n                    <input type=\"time\" id=\"time-start\" class=\"form-input\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"date-end\">Au</label>\r\n                    <input type=\"date\" id=\"date-end\" class=\"form-input\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"time-end\">À :</label>\r\n                    <input type=\"time\" id=\"time-end\" class=\"form-input\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"motif\">Motif</label>\r\n                    <select id=\"motif\" class=\"form-input\" required>\r\n                        <option value=\"\">Choisissez le motif</option>\r\n                        <option value=\"maladie\">Maladie</option>\r\n                        <option value=\"rendez-vous\">Rendez-vous</option>\r\n                        <option value=\"autre\">Autre</option>\r\n                    </select>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"commentaire\">Commentaire</label>\r\n                    <textarea id=\"commentaire\" class=\"form-input\" placeholder=\"Écrivez un commentaire... (facultatif)\"></textarea>\r\n                </div>\r\n            </div>\r\n            <div class=\"form-section\">\r\n                <h4>Renseignements/Documents</h4>\r\n                <div class=\"form-group\">\r\n                    <label for=\"justificatif\" class=\"file-label\">Importer un fichier</label>\r\n                    <input type=\"file\" id=\"justificatif\" class=\"file-input\">\r\n                    <span class=\"error-message\"></span>\r\n                    <small>Fichier justificatif (pdf, png, jpeg, jpg)</small>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"nom\">Nom</label>\r\n                    <input type=\"text\" id=\"nom\" class=\"form-input\" placeholder=\"Entrez votre nom...\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"prenom\">Prénom</label>\r\n                    <input type=\"text\" id=\"prenom\" class=\"form-input\" placeholder=\"Entrez votre prénom...\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n                <div class=\"form-group\">\r\n                    <label for=\"email\">Adresse mail</label>\r\n                    <input type=\"email\" id=\"email\" class=\"form-input\" placeholder=\"Entrez votre adresse mail...\" required>\r\n                    <span class=\"error-message\"></span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <div class=\"button-footer\">\r\n            <button id=\"submit-justify-btn\" class=\"action-button\">Envoyer</button>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<script>\r\n    const justifyModal = document.getElementById('justify-modal');\r\n    const depositJustifyBtn = document.getElementById('deposit-justify-btn');\r\n    const closeModalBtn = justifyModal.querySelector('.close-button');\r\n    const submitJustifyBtn = document.getElementById('submit-justify-btn');\r\n\r\n    depositJustifyBtn.addEventListener('click', () => {\r\n        justifyModal.style.display = 'flex';\r\n    });\r\n\r\n    closeModalBtn.addEventListener('click', () => {\r\n        justifyModal.style.display = 'none';\r\n    });\r\n\r\n    window.addEventListener('click', (event) => {\r\n        if (event.target === justifyModal) {\r\n            justifyModal.style.display = 'none';\r\n        }\r\n    });\r\n\r\n    submitJustifyBtn.addEventListener('click', (event) => {\r\n        event.preventDefault();\r\n\r\n        const requiredFields = document.querySelectorAll('#justify-modal [required]');\r\n        let formIsValid = true;\r\n\r\n        requiredFields.forEach(field => {\r\n            const errorSpan = field.nextElementSibling;\r\n            if (!field.value) {\r\n                field.classList.add('invalid');\r\n                errorSpan.textContent = \"Ce champ est obligatoire\";\r\n                formIsValid = false;\r\n            } else {\r\n                field.classList.remove('invalid');\r\n                errorSpan.textContent = \"\";\r\n            }\r\n        });\r\n\r\n        if (formIsValid) {\r\n            alert('Formulaire envoyé avec succès !');\r\n            justifyModal.style.display = 'none';\r\n        }\r\n    });\r\n</script>\r\n</body>\r\n</html>\r\n
